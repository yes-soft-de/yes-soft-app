<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="de.yessoft.android">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".application.YesSoft"
        android:allowBackup="true"
        android:icon="@drawable/ic_yes_soft_logo"
        android:label="@string/app_name"
        android:roundIcon="@drawable/ic_yes_soft_logo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".activity.BlogsActivity"></activity>
        <activity android:name=".activity.ServicesActivity" />
        <activity android:name=".activity.EmployeesActivity" />
        <activity android:name=".activity.MainActivity.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>